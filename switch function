#include <stdio.h>
#include <math.h>

int main() {
    char grade;

    printf("enter your grade ? ");
    scanf("%c", &grade);

    switch(grade){
     case'A' :
        printf("Perfect");
        break;
     case'B' :
        printf("you did good");
        break;
     case'C' :
        printf("you did okey");
        break;
     case'D' :
        printf("you did not good");
        break;
    default :
        printf("error , try again ");
    }

    return 0;

}
