#include <stdio.h>
#include <math.h>

int main() {

    int num1 , num2 ;

    printf("Enter the first number : ");
    scanf("%f" , &num1);
    printf("Enter the second number : ");
    scanf("%f" , &num2);

    int resultat = num1 % num2;

    printf("the result is : %d\n" , resultat);

    return 0;
