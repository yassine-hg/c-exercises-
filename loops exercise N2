#include <stdio.h>
#include <stdlib.h>

int main() {
    int num ;
    int win = 5 ;


    printf("Enter un number entre 10 et 20 : ");
    scanf("%d" , &num);

    while(num < 10 || num > 20){

        if (num < win){
            printf("Plus grand\n");
        }
        else if(num > win ){
            printf("Plus petit\n");
        }
        else if (num == win){
            printf("Gagne !!\n");
            break;
        }
        else{
            printf("error\n");
            
        }
        printf("Enter un number entre 10 et 20 : ");
        scanf("%d" , &num);

    }


return 0;
